<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    
    <!--General information-->
    <Description>A music utility library for Discord.Net</Description>
    <PackageTags>Discord.Net, Music, Opus</PackageTags>
    <Version>1.0.5</Version>
    
    <!--Type is git by default-->
    <RepositoryUrl>https://github.com/ComputerMaster1st/Shared.Music</RepositoryUrl>
    <Company>CryoDev Development Team</Company>
    <Authors>CryoDev Development Team</Authors>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="AudioChord.Parsers" Version="1.0.2" />
    <PackageReference Include="MongoDB.Driver" Version="2.7.3" />
    <PackageReference Include="MongoDB.Driver.GridFS" Version="2.7.3" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.1" />
    <!--This package should work with all hotfixes-->
    <PackageReference Include="YoutubeExplode" Version="4.6.6" />
  </ItemGroup>
</Project>